{
  "name": "BrowserBricks",
  "version": "1.0.0",
  "description": "BrowserBricks",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . BrowserBricks --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName= --version-string.FileDescription= --version-string.ProductName=\"BrowserBricks\"",
    "package-win-large": "electron-packager . BrowserBricks --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds-large --version-string.CompanyName= --version-string.FileDescription= --version-string.ProductName=\"BrowserBricks\"",
    "package-linux": "electron-packager . BrowserBricks --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": "",
  "keywords": [
    "Electron"
  ],
  "author": "HowardLiu",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.7.8"
  },
  "dependencies": {
    "@shopify/draggable": "^1.0.0-beta.4",
    "dialogs": "^1.1.20"
  }
}
